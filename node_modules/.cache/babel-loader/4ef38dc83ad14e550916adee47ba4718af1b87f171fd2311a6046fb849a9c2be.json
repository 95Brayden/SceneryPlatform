{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      loginForm: {\n        username: '',\n        password: ''\n      }\n    };\n  },\n  methods: {\n    handleLogin() {\n      console.log(this.loginForm);\n      this.$message.success('登录成功！欢迎来到武汉市旅游景点可视化平台\\n即将跳转到首页...'); // 显示成功消息\n      setTimeout(() => {\n        this.$router.push('/home'); // Navigation to home after successful login\n      }, 2000); // 延迟2秒后跳转，给用户时间看提示消息\n    }\n  }\n};","map":{"version":3,"names":["name","data","loginForm","username","password","methods","handleLogin","console","log","$message","success","setTimeout","$router","push"],"sources":["E:\\Desktop\\scenerymonitor\\src\\views\\login.vue"],"sourcesContent":["<template>\r\n    <el-container class=\"login-container\">\r\n      <el-form ref=\"loginForm\" :model=\"loginForm\" class=\"login-form\" label-position=\"top\">\r\n        <h1 class=\"title\">登录</h1>\r\n        <el-form-item label=\"账号\" prop=\"username\">\r\n          <el-input v-model=\"loginForm.username\" autocomplete=\"off\" placeholder=\"请输入账号\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\" prop=\"password\">\r\n          <el-input type=\"password\" v-model=\"loginForm.password\" autocomplete=\"off\" placeholder=\"请输入密码\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"handleLogin\" class=\"login-button\">登录</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-container>\r\n  </template>\r\n\r\n  <script>\r\n  export default {\r\n    name: 'Login',\r\n    data() {\r\n      return {\r\n        loginForm: {\r\n          username: '',\r\n          password: ''\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      handleLogin() {\r\n        console.log(this.loginForm);\r\n        this.$message.success('登录成功！欢迎来到武汉市旅游景点可视化平台\\n即将跳转到首页...');  // 显示成功消息\r\n        setTimeout(() =>